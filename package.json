{
  "name": "react-redux-example",
  "version": "1.0.0",
  "description": "Simple React Redux Example use",
  "main": "index.html",
  "scripts": {
    "dev": "webpack-dev-server --colors --inline --content-base public/",
    "build": "webpack -p --config ./webpack.config.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipatate/reactReduxExample.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ipatate/reactReduxExample/issues"
  },
  "homepage": "https://github.com/ipatate/reactReduxExample#readme",
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015-loose",
      "babel-preset-stage-1"
    ],
    "compact": true,
    "plugins": [
      "transform-object-assign",
      "transform-proto-to-assign"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "compression-webpack-plugin": "^0.3.2",
    "i18next": "^3.3.1",
    "preact": "^7.1.0",
    "preact-compat": "^3.9.4",
    "react": "^15.2.0",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-i18next": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
